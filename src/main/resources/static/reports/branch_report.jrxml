	<parameter name="start_date" class="java.lang.String"/>
	<parameter name="end_date" class="java.lang.String"/>
	<parameter name="branch" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT SUM(bazar) AS bazars, SUM(ebill) AS ebills,  SUM(salary) AS salaries,  SUM(bill) AS bills,  SUM(cost) AS costs, SUM(deposits) as deposit, SUM(fees) as fee, ((SUM(deposits)+SUM(fees))-(SUM(bazar)+SUM(ebill)+SUM(salary)+SUM(bill)+SUM(cost))) AS profit, (SELECT name FROM branch WHERE id = $P{branch}) AS branch, (SELECT name FROM hostel WHERE id = (SELECT hostel FROM branch WHERE id = $P{branch})) AS hostel
FROM(
SELECT SUM(total) AS bazar, 0 AS ebill, 0 AS salary, 0 AS bill, 0 AS cost, 0 AS fees, 0 AS deposits FROM Bazar b WHERE b.date BETWEEN $P{start_date} and $P{end_date} and b.branch = $P{branch}
UNION ALL
SELECT 0 AS bazar, SUM(total) AS ebill, 0 AS salary, 0 AS bill, 0 AS cost, 0 AS fees, 0 AS deposits FROM Electricity_Bills b  WHERE b.date BETWEEN $P{start_date} and $P{end_date} and b.branch = $P{branch}
UNION ALL
SELECT 0 AS bazar, 0 AS ebill, SUM(amount) AS salary, 0 AS bill, 0 AS cost, 0 AS fees, 0 AS deposits FROM Salary b WHERE b.date BETWEEN $P{start_date} and $P{end_date} and b.employee in (SELECT id FROM employee WHERE branch = $P{branch} AND is_active = true)
UNION ALL
SELECT 0 AS bazar, 0 AS ebill, 0 AS salary, SUM(amount) AS bill, 0 AS cost, 0 AS fees, 0 AS deposits FROM Bills b WHERE b.date BETWEEN $P{start_date} and $P{end_date} and b.branch = $P{branch}
UNION ALL
SELECT 0 AS bazar, 0 AS ebill, 0 AS salary, 0 AS bill, SUM(amount) AS cost, 0 AS fees, 0 AS deposits FROM Cost b WHERE b.date BETWEEN $P{start_date} and $P{end_date} and b.branch = $P{branch}
UNION ALL
SELECT 0 AS bazar, 0 AS ebill, 0 AS salary, 0 AS bill, 0 AS cost, SUM(amount) AS fees, 0 AS deposits FROM Student_Payment b WHERE b.date BETWEEN $P{start_date} and $P{end_date} and student IN(SELECT e.id from Student e WHERE e.branch = $P{branch})
UNION ALL
SELECT 0 AS bazar, 0 AS ebill, 0 AS salary, 0 AS bill, 0 AS cost, 0 AS fees, SUM(amount) AS deposits FROM Deposit b  WHERE b.date BETWEEN $P{start_date} and $P{end_date} and b.branch = $P{branch}
) AS fs;]]>
	</queryString>
